type vendor_tcpdump, domain;
type vendor_tcpdump_exec, exec_type, file_type, vendor_file_type;

allow vendor_tcpdump self:capability { net_raw setgid setuid };
allow vendor_tcpdump self:packet_socket { bind create getopt ioctl map read setopt write };
allow vendor_tcpdump self:netlink_generic_socket create;
allow vendor_tcpdump self:netlink_generic_socket ioctl;
allow vendor_tcpdump sysfs_mm:dir search;
allow vendor_tcpdump vendor_wifi_vendor_data_file:dir { add_name search write };
allow vendor_tcpdump vendor_wifi_vendor_data_file:file { create getattr open write };
allow vendor_tcpdump sysfs:file { open read };
allow vendor_tcpdump sysfs_net:dir search;
init_daemon_domain(vendor_tcpdump)
