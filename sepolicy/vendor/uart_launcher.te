type uart_launcher, domain;
type uart_launcher_exec, exec_type, file_type, vendor_file_type;

allow uart_launcher stpbt_device:chr_file { append getattr ioctl lock open read watch watch_reads write };
allow uart_launcher ttyS_device:chr_file { append getattr ioctl lock open read watch watch_reads write };
allow uart_launcher proc_stpbt:file r_file_perms;
allow uart_launcher proc_stpbt:dir search;
init_daemon_domain(uart_launcher)
set_prop(uart_launcher, vendor_mtk_wmt_prop)
