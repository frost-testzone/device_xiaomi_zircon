# Audio
genfscon sysfs /class/smartpa/re25_calib u:object_r:sysfs_re25_calib:s0

# Battery
genfscon sysfs /devices/platform/soc/11280000.i2c/i2c-5/5-0034/11280000.i2c:mt6375@34:mtk_gauge/power_supply/battery u:object_r:sysfs_usb_supply:s0
genfscon sysfs /devices/platform/charger/power_supply/usb u:object_r:sysfs_usb_supply:s0
genfscon sysfs /devices/platform/pd_adapter/Charging_Adapter/pd_adapter u:object_r:sysfs_usbpd_device:s0

# NFC
genfscon sysfs /devices/platform/soc/11e80000.i2c/i2c-3/3-0028/chip_name u:object_r:sysfs_chip_name:s0

# Perf
genfscon proc /perfmgr/global_reclaim u:object_r:vendor_proc_perfmgr_global_reclaim:s0

# TEE
genfscon proc /mitee_log u:object_r:proc_mitee_log:s0

